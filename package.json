{
  "name": "ngx-flex-messages",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "pack": "cd dist/ngx-flex-messages && npm pack",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "storybook:build": "npm run docs:json && build-storybook",
    "storybook:deploy": "gh-pages -d storybook-static",
    "publish:prebuild": "lerna run build",
    "publish:version": "lerna version prerelease --conventional-prerelease --exact --preid=alpha.$BUILD_ID --yes --no-push --no-git-tag-version",
    "publish:canary": "npm run publish:version && npm run publish:prebuild && lerna publish --contents dist prerelease --preid=alpha.$BUILD_ID --exact --skip-git --yes",
    "publish:release": "lerna version --create-release github --conventional-commits --exact --yes --message \"chore(release): publish /skip-deploy\" && lerna publish from-git --yes --contents dist"
  },
  "workspaces": [
    "projects/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Guutong/ngx-flex-messages.git"
  },
  "homepage": "http://guutong.github.io/ngx-flex-messages",
  "private": false,
  "dependencies": {
    "@angular/animations": "~12.0.3",
    "@angular/common": "~12.0.3",
    "@angular/compiler": "~12.0.3",
    "@angular/core": "~12.0.3",
    "@angular/forms": "~12.0.3",
    "@angular/platform-browser": "~12.0.3",
    "@angular/platform-browser-dynamic": "~12.0.3",
    "@angular/router": "~12.0.3",
    "rxjs": "~6.6.0",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.3",
    "@angular/cli": "~12.0.3",
    "@angular/compiler-cli": "~12.0.3",
    "@angular/elements": "^12.0.3",
    "@babel/core": "^7.14.3",
    "@compodoc/compodoc": "^1.1.11",
    "@storybook/addon-actions": "^6.3.0-beta.14",
    "@storybook/addon-essentials": "^6.3.0-beta.14",
    "@storybook/addon-links": "^6.3.0-beta.14",
    "@storybook/angular": "^6.3.0-beta.14",
    "@storybook/builder-webpack5": "^6.3.0-beta.14",
    "@storybook/manager-webpack5": "^6.3.0-beta.14",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@webcomponents/custom-elements": "^1.4.3",
    "babel-loader": "^8.2.2",
    "gh-pages": "^3.2.0",
    "jasmine-core": "~3.7.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lerna": "^4.0.0",
    "ng-packagr": "^12.0.0",
    "storybook-addon-angular-ivy": "0.0.1",
    "typescript": "~4.2.3"
  }
}
